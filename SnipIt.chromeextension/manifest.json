{
  "manifest_version": 2,

  "name": "SnipIt",
  "version": "1.0",
  "description": "SnipIt allows you to easily save and organize code snippets in the cloud.",
  "homepage_url": "http://snipit.xyz",
  "icons": {
    "64": "icons/logo-64.png",
    "128": "icons/logo-128.png",
    "512": "icons/logo-512.png"
  },

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/snip-it.js"],
    "css": ["css/snip-it.css"]
  }],

  "web_accessible_resources": [
    "js/snip-it.js",
    "css/snip-it.css"
  ]
}